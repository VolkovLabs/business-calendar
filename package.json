{
  "author": "Volkov Labs",
  "bugs": "https://github.com/volkovlabs/volkovlabs-calendar-panel/issues",
  "description": "A calendar panel for Grafana",
  "devDependencies": {
    "@grafana/data": "9.1.6",
    "@grafana/runtime": "9.1.6",
    "@grafana/toolkit": "9.1.6",
    "@grafana/ui": "9.1.6",
    "@tippyjs/react": "^4.1.0",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "dayjs": "^1.11.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "grafana-plugin-support": "0.4.0",
    "react-virtualized-auto-sizer": "^1.0.7"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "name": "marcusolsson-calendar-panel",
  "scripts": {
    "build": "grafana-toolkit plugin:build --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "format": "prettier --write \"**\"",
    "sign": "grafana-toolkit plugin:sign",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "grafana-toolkit plugin:test",
    "upgrade": "yarn upgrade --latest",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "version": "1.1.0"
}
