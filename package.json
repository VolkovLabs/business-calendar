{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "^10.1.4",
    "@grafana/runtime": "^10.1.4",
    "@grafana/ui": "^10.1.4",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "dayjs": "^1.11.10",
    "i18next": "^23.6.0",
    "react": "18.2.0",
    "react-big-calendar": "^1.10.3",
    "react-dom": "18.2.0",
    "react-i18next": "^13.3.1",
    "react-virtualized-auto-sizer": "^1.0.20",
    "tslib": "^2.6.2"
  },
  "description": "A Calendar Panel for Grafana",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@grafana/e2e": "^10.1.4",
    "@grafana/e2e-selectors": "^10.1.4",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.92",
    "@swc/helpers": "^0.5.2",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.18.3",
    "@types/react-big-calendar": "^1.8.8",
    "@types/webpack-env": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@volkovlabs/jest-selectors": "^1.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-calendar-panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "2.3.1"
}
